{
    "name": "silverstripe/recipe-blog",
    "type": "silverstripe-recipe",
    "description": "SilverStripe Blog Project Template",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "1.1.0-rc2",
        "silverstripe/blog": "3.0.0@stable",
        "silverstripe/widgets": "2.0.0@stable",
        "silverstripe/content-widget": "2.0.0@stable",
        "silverstripe/spamprotection": "3.0.0@stable",
        "silverstripe/akismet": "4.0.0@stable",
        "silverstripe/comments": "3.0.0@stable",
        "silverstripe/comment-notifications": "2.0.0@stable",
        "colymba/gridfield-bulk-editing-tools": "3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files": [
            "mysite/*"
        ],
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}